   "source": [
    "# NamedTuple Model\n",
    "ModelInf = namedtuple(\"Model\", (\"M_grid\", \"β\", \"R\"))\n",
    "\n",
    "\n",
    "def create_cake_eating_model_inf(\n",
    "        n_M=100,\n",
    "        M_min=1e-3,\n",
    "        M_max=10,\n",
    "        β=0.95,\n",
    "        R=1.05,\n",
    "    ):\n",
    "\n",
    "    M_grid = np.linspace(M_min, M_max, n_M+1)\n",
    "\n",
    "    return ModelInf(M_grid=M_grid, β=β, R=R)"
   ]
  },

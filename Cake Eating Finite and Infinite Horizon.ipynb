    "def vfi(model):\n",
    "    v_init = np.zeros(model.M_grid.size)\n",
    "    v_star = compute_fixed_point(lambda v: policy_egm(model), v_init,\n",
    "                                 error_tol=1e-5, max_iter=1000,\n",
    "                                 verbose=0)\n",
    "    return v_star"

   "metadata": {},
   "outputs": [],
   "source": [
    "ModelFinite = namedtuple(\"Model\", (\"M_grid\", \"β\", \"R\", \"T\")) #Create a tuple labeled model with parameters\n",
    "\n",
    "def create_cake_eating_model( #define the cake eating model with values for the parameters\n",
    "        n_M=101,\n",
    "        M_min=1e-3,\n",
    "        M_max=10,\n",
    "        r= [1.04, 1.05, 1.06],\n",
    "        prob = [(1/3), (1/3), (1/3)],\n",
    "        β=0.95,\n",
    "        distrib = rv_discrete(values= (r, prob)),\n",
    "        T=10\n",
    "    ):\n",
    "    M_grid = np.linspace(M_min, M_max, n_M+1) #Create grid of the cake\n",
    "    R = ((1/3) * 1.04) + ((1/3)*1.05) + ((1/3)*1.06) #Expectation of R\n",
    "    return ModelFinite(M_grid=M_grid, β=β, R=R, T=T) #fill the tuple above with the values"
   ]

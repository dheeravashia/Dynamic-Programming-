    "@njit\n",
    "def U_prime(c):\n",
    "\n",
    "    return 1 / c"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 151,
   "id": "9048f24d",
   "metadata": {},
   "outputs": [],
   "source": [
    "@njit\n",
    "def U_prime_inv(v):\n",
    "\n",
    "    return 1 / v"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 198,
   "id": "1ce3f74c",
   "metadata": {},
   "outputs": [],
   "source": [
    "@njit\n",
    "def σc_egm(c_next, model):\n",
    "\n",
    "    M_grid, β, R = model\n",
    "    uprime_next = U_prime(c_next)\n",
    "    c_grid = U_prime_inv(R * β * uprime_next)\n",
    "    A_grid = M_grid / R\n",
    "    M_t = A_grid + c_grid\n",
    "\n",
    "    return interp(M_t, c_grid, M_grid)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 268,
   "id": "25c1639b",
   "metadata": {},
   "outputs": [],
   "source": [
    "def policy_egm(model):\n",
    "\n",
    "    M_grid, β, R = model\n",
    "\n",
    "    c_arr = np.empty(M_grid.size)\n",
    "    \n",
    "    for i, c in enumerate(M_grid):\n",
    "        c_arr[i] = σc_egm(c_arr[i+1], model) #I want to create an array that guess a cerain random consumpition for the individual.\n",
    "\n",
    "    return c_arr"
   ]
  },

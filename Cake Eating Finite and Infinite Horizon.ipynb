    "@njit \n",
    "def U(c): #Define the utility function \n",
    "\n",
    "    return np.log(c)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 103,
   "id": "35fcd672",
   "metadata": {},
   "outputs": [],
   "source": [
    "@njit\n",
    "def c_val(c, M, V_next, model):\n",
    "\n",
    "    M_grid, β, R, T = model\n",
    "\n",
    "    val = U(c) + β * interp(M_grid, V_next, (R)*(M-c))\n",
    "\n",
    "    return val"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 104,
   "id": "a74a3248",
   "metadata": {},
   "outputs": [],
   "source": [
    "@njit\n",
    "def V_t(M, V_next, model):\n",
    "\n",
    "    M_grid, β, R, T = model\n",
    "\n",
    "    c_opt, V_opt, _ = brent_max(c_val, 1e-6, M-1e-6, args=(M, V_next, model))\n",
    "\n",
    "    return V_opt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 105,
   "id": "72854017",
   "metadata": {},
   "outputs": [],
   "source": [
    "@njit\n",
    "def backwards_induction(model):\n",
    "\n",
    "    M_grid, β, R, T = model\n",
    "\n",
    "    V_arr = np.empty((T, M_grid.size))\n",
    "\n",
    "    V_arr[T-1] = U(M_grid)\n",
    "\n",
    "    for t in range(T-2, -1, -1):\n",
    "        V_arr[t] = [V_t(M, V_arr[t+1], model) for M in M_grid]\n",
    "\n",
    "    return V_arr"
   ]
  },
